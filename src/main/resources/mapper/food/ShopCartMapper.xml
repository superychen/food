<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqyc.food.mapper.ShopCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cqyc.food.domain.ShopCart">
        <id column="id" property="id" />
        <result column="fID" property="fID" />
        <result column="buyerName" property="buyerName" />
        <result column="fName" property="fName" />
        <result column="fImage" property="fImage" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        fID, buyerName, id, fName, fImage
    </sql>

</mapper>
